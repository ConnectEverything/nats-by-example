{
  "public": false,
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "use": "@vercel/static",
      "src": "./**"
    }
  ],
  "redirects": [
    {
      "source": "/examples/micro/(.*)",
      "destination": "/examples/services/$1",
      "permanent": true
    }
  ],
  "cleanUrls": true,
  "trailingSlash": true
}
